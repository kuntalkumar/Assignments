{
    "name": "whatsapp sender",
    "version": "0.0.1",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "index.html",
        "default_icon": "whatsapp.png"
    },
    "permissions": [
        "activeTab",
        "tabs"
    ],
    
    "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'",
    "host_permissions": [
        "https://wa.me/*"
    ]
}
